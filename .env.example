# PackageFlow OAuth Configuration
# Copy this file to .env and fill in your credentials

# Vercel OAuth (from https://vercel.com/dashboard -> Integrations -> Integrations Console)
PACKAGEFLOW_VERCEL_CLIENT_ID=oac_xxxxxxxxxx
PACKAGEFLOW_VERCEL_CLIENT_SECRET=xxxxxxxxxx

# Netlify OAuth (from https://app.netlify.com/user/applications)
PACKAGEFLOW_NETLIFY_CLIENT_ID=xxxxxxxxxx
