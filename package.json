{
  "name": "packageflow",
  "private": true,
  "license": "mit",
  "version": "0.3.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "tsc && vite build",
    "build:tauri": "tauri build --bundles dmg",
    "build:mcp": "cargo build -p packageflow-mcp && node scripts/ensure-mcp-alias.mjs --profile=debug",
    "build:mcp:release": "[ -n \"$CI\" ] && [ -f target/release/packageflow-mcp ] && echo 'Skipping MCP build in CI (pre-built and signed)' || (cargo build -p packageflow-mcp --release && node scripts/ensure-mcp-alias.mjs --profile=release)",
    "prebuild:tauri": "pnpm test:mcp && pnpm build:mcp:release",
    "predev:tauri": "pkill -f packageflow-mcp || true; pnpm build:mcp",
    "preview": "vite preview",
    "tauri": "tauri",
    "test:mcp": "cargo test -p packageflow-mcp",
    "test:mcp:verbose": "cargo test -p packageflow-mcp -- --nocapture",
    "test:mcp:watch": "cargo watch -x 'test -p packageflow-mcp'",
    "clean": "cargo clean",
    "kill": "lsof -ti:1420 | xargs kill -9",
    "lint": "eslint src/ --max-warnings 0",
    "lint:fix": "eslint src/ --fix",
    "lint:rust": "cargo clippy --manifest-path src-tauri/Cargo.toml -- -D warnings",
    "lint:all": "pnpm lint && pnpm lint:rust",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "audit": "pnpm audit",
    "audit:fix": "pnpm audit --fix",
    "version:bump": "./scripts/bump-version.sh",
    "version:check": "./scripts/bump-version.sh --check"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-virtual": "^3.13.13",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "^2",
    "@tauri-apps/plugin-global-shortcut": "^2",
    "@tauri-apps/plugin-notification": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "~2.3.1",
    "@tauri-apps/plugin-shell": "^2",
    "@tauri-apps/plugin-store": "^2",
    "@tauri-apps/plugin-updater": "~2.9.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "@xyflow/react": "^12.10.0",
    "ansi-to-html": "^0.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "marked": "^17.0.1",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "strip-ansi": "^7.1.2",
    "tailwind-merge": "^3.4.0",
    "tauri-pty": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/cli": "^2",
    "@types/marked": "^6.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.2.7"
  }
}
