{
  "name": "packageflow",
  "private": true,
  "license": "mit",
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "tsc && vite build",
    "build:tauri": "tauri build --bundles dmg",
    "build:mcp": "cargo build --manifest-path src-tauri/Cargo.toml --bin packageflow-mcp",
    "build:mcp:release": "cargo build --manifest-path src-tauri/Cargo.toml --release --bin packageflow-mcp",
    "prebuild:tauri": "pnpm test:mcp && pnpm build:mcp:release",
    "predev:tauri": "pkill -f packageflow-mcp || true; pnpm build:mcp",
    "preview": "vite preview",
    "tauri": "tauri",
    "test:mcp": "cargo test --manifest-path src-tauri/Cargo.toml --bin packageflow-mcp mcp_tests",
    "clean": "cargo clean --manifest-path src-tauri/Cargo.toml",
    "kill": "lsof -ti:1420 | xargs kill -9"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-virtual": "^3.13.13",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "^2",
    "@tauri-apps/plugin-global-shortcut": "^2",
    "@tauri-apps/plugin-notification": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-process": "~2.3.1",
    "@tauri-apps/plugin-shell": "^2",
    "@tauri-apps/plugin-store": "^2",
    "@tauri-apps/plugin-updater": "~2.9.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "@xyflow/react": "^12.10.0",
    "ansi-to-html": "^0.7.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.556.0",
    "marked": "^17.0.1",
    "path-browserify": "^1.0.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "strip-ansi": "^7.1.2",
    "tailwind-merge": "^3.4.0",
    "tauri-pty": "^0.1.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tauri-apps/cli": "^2",
    "@types/marked": "^6.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "vite": "^7.2.7"
  }
}
